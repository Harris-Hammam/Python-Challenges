def AND (a, b):
    if a == 1 and b == 1:
        return True
    else:
        return False

def OR (a, b):
    if a == 1 or b == 1:
        return True
    else:
        return False

def NOR (a,b):
    if a == 1 or b == 1:
        return False
    else:
        return True

def NOT (a):
    if a == 1:
        return False
    else:
        return True

def XOR (a, b):
    if a == 1 and b == 1:
        return False
    elif a == 0 and b == 0:
        return False
    else:
        return True
