import turtle
wn = turtle.Screen()
tess = turtle.Turtle()

for i in range (3):
    turtle.forward(90)
    turtle.left(120)

for i in range(4):
    turtle.forward(90)
    turtle.left(90)

for i in range(6):
    turtle.forward(90)
    turtle.left(60)

for i in range(8):
    turtle.forward(90)
    turtle.left(45)
